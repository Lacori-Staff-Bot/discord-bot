{
  "name": "discord-bot",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc",
    "build:changelog": "npx @discordx/changelog --src src",
    "dev": "node --loader ts-node/esm/transpile-only src/main.ts",
    "start": "node build/main.js",
    "watch": "nodemon --exec npm run dev"
  },
  "dependencies": {
    "@discordx/importer": "^1.3.0",
    "discord.js": "^14.14.1",
    "discordx": "^11.9.0",
    "express": "^4.18.2",
    "mysql2": "^3.6.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
